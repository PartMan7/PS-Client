    "juice": "^9.1.0",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0"
  }
}
