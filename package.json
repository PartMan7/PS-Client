{
  "name": "ps-client",
  "homepage": "https://github.com/PartMan7/PS-Client#readme",
  "version": "3.3.2",
  "description": "Package for connecting to Pokemon Showdown servers.",
  "main": "client.js",
  "scripts": {
    "lint": "eslint . --config .eslintrc.json *.js",
    "mocha": "mocha test/*.js -t 10000",
